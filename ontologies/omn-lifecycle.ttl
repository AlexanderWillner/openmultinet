###  http://open-multinet.info/ontology/omn-lifecycle#hasReservationState

:hasReservationState rdf:type owl:FunctionalProperty ,
                              owl:IrreflexiveProperty ,
                              owl:ObjectProperty ;

                     rdfs:comment "The state of the reservation."@en ;
                     
                     rdfs:domain omn:Reservation ;
                     
                     rdfs:range :ReservationState ;
                     
                     rdfs:subPropertyOf :hasState .



###  http://open-multinet.info/ontology/omn-lifecycle#hasState

:hasState rdf:type owl:FunctionalProperty ,
                   owl:IrreflexiveProperty ,
                   owl:ObjectProperty ;

          rdfs:comment "Has state."@en ;
          
          rdfs:range :State ;
          
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( omn:Component
                                      omn:Group
                                      omn:Resource
                                      omn:Service
                                    )
                      ] .

###  http://open-multinet.info/ontology/omn-lifecycle#usesService
:usesService rdf:type 
                   owl:IrreflexiveProperty ,
                   owl:ObjectProperty ;
          
          rdfs:comment "Uses a service."@en ;

          rdfs:range omn:Service ;
          
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( omn:Component
                                      omn:Group
                                      omn:Resource
                                      omn:Service
                                    )
                      ] .
                      
                      
###  http://open-multinet.info/ontology/omn-lifecycle#canImplement

:canImplement rdf:type owl:ObjectProperty ;

            rdfs:comment "That which does not currently implement, but can potentially implement a resource, service, component or group."@en ;
            
            owl:inverseOf :canBeImplementedBy ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( omn:Component
                                        omn:Group
                                        omn:Resource
                                        omn:Service
                                      )
                        ] ;
            
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( omn:Component
                                       omn:Group
                                       omn:Resource
                                       omn:Service
                                     )
                       ] .


###  http://open-multinet.info/ontology/omn-lifecycle#canBeImplementedBy

:canBeImplementedBy rdf:type owl:ObjectProperty ;

               rdfs:comment "That which is not currently implemented, but can potentially be implemented by a resource, service, component or group."@en ;
               
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( omn:Component
                                          omn:Group
                                          omn:Resource
                                          omn:Service
                                        )
                          ] ;
               
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( omn:Component
                                           omn:Group
                                           omn:Resource
                                           omn:Service
                                         )
                           ] .


###  http://open-multinet.info/ontology/omn-lifecycle#implementedBy

:implementedBy rdf:type owl:ObjectProperty ;

               rdfs:label "implementd by."@en ;

               rdfs:comment "Implementd by."@en ;
               
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( omn:Component
                                          omn:Group
                                          omn:Resource
                                          omn:Service
                                        )
                          ] ;
               
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( omn:Component
                                           omn:Group
                                           omn:Resource
                                           omn:Service
                                         )
                           ] .



###  http://open-multinet.info/ontology/omn-lifecycle#implements

:implements rdf:type owl:ObjectProperty ;
            
            rdfs:comment "Implements."@en ;

            owl:inverseOf :implementedBy ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( omn:Component
                                        omn:Group
                                        omn:Resource
                                        omn:Service
                                      )
                        ] ;
            
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( omn:Component
                                       omn:Group
                                       omn:Resource
                                       omn:Service
                                     )
                       ] .



###  http://open-multinet.info/ontology/omn-lifecycle#isReservationStateOf

:isReservationStateOf rdf:type owl:ObjectProperty ;

                      rdfs:label "is reservation state of."@en ;

                      rdfs:comment "is reservation state of."@en ;
                      
                      rdfs:range omn:Reservation ;
                      
                      rdfs:domain :ReservationState ;
                      
                      rdfs:subPropertyOf :isStateOf .



###  http://open-multinet.info/ontology/omn-lifecycle#isStateOf

:isStateOf rdf:type owl:ObjectProperty ;

           rdfs:comment "is state of."@en ;

           rdfs:label "is state of."@en ;
           
           rdfs:domain :State ;
           
           owl:inverseOf :hasState ;
           
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( omn:Component
                                      omn:Group
                                      omn:Resource
                                      omn:Service
                                    )
                      ] .



###  http://open-multinet.info/ontology/omn-lifecycle#parentOf

:parentOf rdf:type owl:ObjectProperty ;

          rdfs:comment "A resource, component, group or serice which encompasses another."@en ;
          
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( omn:Component
                                     omn:Group
                                     omn:Resource
                                     omn:Service
                                   )
                     ] ;
          
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( omn:Component
                                      omn:Group
                                      omn:Resource
                                      omn:Service
                                    )
                      ] .



###  http://open-multinet.info/ontology/omn-lifecycle#childOf

:childOf rdf:type owl:ObjectProperty ;

          rdfs:comment "A resource, component, group or serice which is a part of another."@en ;
          
          owl:inverseOf :parentOf ;
          
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( omn:Component
                                      omn:Group
                                      omn:Resource
                                      omn:Service
                                    )
                      ];
                                
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( omn:Component
                                     omn:Group
                                     omn:Resource
                                     omn:Service
                                   )
                     ] .





