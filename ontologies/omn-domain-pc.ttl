###  http://open-multinet.info/ontology/omn-domain-pc#hasDiskImage

:hasDiskImage rdf:type owl:ObjectProperty ;
            
            rdfs:label "has disk image"@en ;

            rdfs:comment "has disk image"@en ;
            
            rdfs:range :DiskImage ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( omn:Group
                                        omn:Resource
                                        omn:Service
                                      )
                        ] ;

            owl:inverseOf :isDiskImageOf .


###  http://open-multinet.info/ontology/omn-domain-pc#hasAvailable

:hasAvailable rdf:type owl:ObjectProperty ;
            
            rdfs:label "has avalable"@en ;

            rdfs:comment "has avalable"@en ;
            
            rdfs:range :Available ;
            
            rdfs:domain :SharedVlan .



###  http://open-multinet.info/ontology/omn-domain-pc#isDiskImageOf

:isDiskImageOf rdf:type owl:ObjectProperty ;
            
            rdfs:label "is disk image of"@en ;

            rdfs:comment "is disk image of"@en ;
            
            rdfs:domain :DiskImage ;
            
            rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( omn:Group
                                        omn:Resource
                                        omn:Service
                                      )
                        ] ;

            owl:inverseOf :hasDiskImage .




###  http://open-multinet.info/ontology/omn-domain-pc#hasHardwareType

:hasHardwareType rdf:type owl:ObjectProperty ;
            
            rdfs:label "has hardware type"@en ;

            rdfs:comment "has hardware type"@en ;
            
            rdfs:range :HardwareType ;
            
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( omn:Group
                                        omn:Resource
                                        omn:Service
                                      )
                        ] ;

            owl:inverseOf :isHardwareTypeOf .



###  http://open-multinet.info/ontology/omn-domain-pc#isHardwareTypeOf

:isHardwareTypeOf rdf:type owl:ObjectProperty ;
            
            rdfs:label "is hardware type of"@en ;

            rdfs:comment "is hardware type of"@en ;
            
            rdfs:domain :HardwareType ;
            
            rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( omn:Group
                                        omn:Resource
                                        omn:Service
                                      )
                        ] ;

            owl:inverseOf :hasHardwareType .


