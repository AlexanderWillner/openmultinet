#################################################################
#
#    Object Properties
#
#################################################################


###  http://federation.av.tu-berlin.de/fiddle#hasInfrastructure

:hasInfrastructure rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has infrastructure" ;
                   
                   rdfs:comment "Defines which infrastructures (aka testbeds) are part of a federation."@en ;
                   
                   rdfs:domain :Federation ;
                   
                   rdfs:range :Infrastructure ;
                   
                   owl:inverseOf :partOfFederation .



###  http://federation.av.tu-berlin.de/fiddle#hasNode

:hasNode rdf:type owl:ObjectProperty ;
         
         rdfs:label "has a node" ;
         
         rdfs:comment "Defines which resources/services belongs to which infrastructure."@en ;
         
         rdfs:domain :Infrastructure ;
         
         owl:inverseOf :partOfInfrastructure .



###  http://federation.av.tu-berlin.de/fiddle#hasService

:hasService rdf:type owl:ObjectProperty ;
            
            rdfs:label "has service" ;
            
            rdfs:seeAlso "http://schemas.ogf.org/nml/2013/05/base#hasService" ;
            
            rdfs:comment "Defines which services a group (e.g. federation or infrastructure) offers (e.g. an GENI SFA Aggregate Manager)."@en ;
            
            rdfs:domain nml:Group ;
            
            rdfs:range nml:Service .



###  http://federation.av.tu-berlin.de/fiddle#partOfFederation

:partOfFederation rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "is part of federation" ;
                  
                  rdfs:comment "Defines which federations an infrastructure belongs to."@en ;
                  
                  rdfs:range :Federation ;
                  
                  rdfs:domain :Infrastructure .



###  http://federation.av.tu-berlin.de/fiddle#partOfInfrastructure

:partOfInfrastructure rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is part of an infrastructure" ;
                      
                      rdfs:comment "Defines which resources/services belongs to which infrastructure."@en ;
                      
                      rdfs:range :Infrastructure .



###  http://xmlns.com/foaf/0.1/homepage

foaf:homepage rdf:type owl:ObjectProperty .



###  http://xmlns.com/foaf/0.1/mbox

foaf:mbox rdf:type owl:ObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://federation.av.tu-berlin.de/fiddle#infraMontoring

:infraMontoring rdf:type owl:DatatypeProperty ;
                
                rdfs:label "has infra monitoring" ;
                
                rdfs:domain <http://schemas.ogf.org/nml/base/2013/02#Node> ;
                
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "no" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "yes" ;
                                                    rdf:rest rdf:nil
                                                  ]
                                       ]
                           ] .



###  http://federation.av.tu-berlin.de/fiddle#omlServer

:omlServer rdf:type owl:DatatypeProperty ;
           
           rdfs:domain <http://schemas.ogf.org/nml/base/2013/02#Node> ;
           
           rdfs:range xsd:anyURI .



###  http://federation.av.tu-berlin.de/fiddle#serviceURI

:serviceURI rdf:type owl:DatatypeProperty ;
            
            rdfs:label "has service URI" ;
            
            rdfs:comment "An endpoint where the service can be reached."@en ;
            
            rdfs:domain nml:Service ;
            
            rdfs:range xsd:anyURI .



###  http://xmlns.com/foaf/0.1/name

foaf:name rdf:type owl:DatatypeProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://federation.av.tu-berlin.de/fiddle#AccountabilityService

:AccountabilityService rdf:type owl:Class ;
                       
                       rdfs:label "Accountability Service" ;
                       
                       rdfs:subClassOf nml:Service ;
                       
                       rdfs:comment "\"The Clearinghouse provides services that log transactions (successful or failed) between user tools and aggregates to support real-time and post-facto forensics analytics.\" http://groups.geni.net/geni/wiki/GeniClearinghouse" .



###  http://federation.av.tu-berlin.de/fiddle#AggregateManager

:AggregateManager rdf:type owl:Class ;
                  
                  rdfs:label "Aggregate Manager"@en ;
                  
                  rdfs:subClassOf nml:Service ;
                  
                  rdfs:comment "An instance of an Aggregate Manager satisfying the Aggregate Manager API."@en .



###  http://federation.av.tu-berlin.de/fiddle#AuthorizationService

:AuthorizationService rdf:type owl:Class ;
                      
                      rdfs:label "Authorization Service" ;
                      
                      rdfs:subClassOf nml:Service ;
                      
                      rdfs:comment "\"The Clearinghouse provides services to determine whether particular actions (within the Clearinghouse or with respect to a particular Aggregate) are permitted by federation policy.\" http://groups.geni.net/geni/wiki/GeniClearinghouse" .



###  http://federation.av.tu-berlin.de/fiddle#CloudComputing

:CloudComputing rdf:type owl:Class ;
                
                rdfs:label "Cloud Computing" ;
                
                rdfs:subClassOf :TechnicalInfrastructure ;
                
                rdfs:seeAlso "http://www.fi-infinity.eu/portal/resources/deliverable/d36" .



###  http://federation.av.tu-berlin.de/fiddle#Federation

:Federation rdf:type owl:Class ;
            
            rdfs:label "Federation" ;
            
            rdfs:subClassOf nml:Group ;
            
            rdfs:comment "A group of autonomous infrastructure/service providers agreeing upon standards of operation in a collective fashion."@en .



###  http://federation.av.tu-berlin.de/fiddle#FederationService

:FederationService rdf:type owl:Class ;
                   
                   rdfs:label "Federation Service" ;
                   
                   rdfs:subClassOf nml:Service ;
                   
                   rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#IdentityProvider

:IdentityProvider rdf:type owl:Class ;
                  
                  rdfs:label "Identity Provider" ;
                  
                  rdfs:subClassOf :FederationService ;
                  
                  rdfs:comment "An Identity Provider (IdP) provides certificates and PKI key materials to human users, registering them with the GENI federation as GENI users." ;
                  
                  rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#Infrastructure

:Infrastructure rdf:type owl:Class ;
                
                rdfs:label "Infrastructure" ;
                
                rdfs:subClassOf nml:Group ;
                
                rdfs:comment "An autonomous infrastrucure that offers resources/services to others within a federation. See also http://www.fi-infinity.eu/portal/resources/deliverable/d36 for discussion of what constitutes an ICT infrastructure."@en .



###  http://federation.av.tu-berlin.de/fiddle#LoggingService

:LoggingService rdf:type owl:Class ;
                
                rdfs:label "Logging Service" ;
                
                rdfs:subClassOf :AccountabilityService ;
                
                rdfs:comment "The Logging Service provides the traceability between slivers and slices." ;
                
                rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#OpenFlowInfrastructure

:OpenFlowInfrastructure rdf:type owl:Class ;
                        
                        rdfs:label "OpenFlow Infrastructure" ;
                        
                        rdfs:subClassOf :TechnicalInfrastructure .



###  http://federation.av.tu-berlin.de/fiddle#ProjectAuthority

:ProjectAuthority rdf:type owl:Class ;
                  
                  rdfs:label "Project Authority" ;
                  
                  rdfs:subClassOf :FederationService ;
                  
                  rdfs:comment "A Project Authority asserts the existence of projects and the roles of members (e.g. PI, Experimenter)." ;
                  
                  rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#ServiceRegistry

:ServiceRegistry rdf:type owl:Class ;
                 
                 rdfs:label "Service Registry" ;
                 
                 rdfs:subClassOf :FederationService ;
                 
                 rdfs:comment "A Service Registry provides experimenters with a ‘yellow pages’ of URL’s of all trusted services of different kinds. In particular, the list of all available aggregate managers trusted by GENI (possibly satisfying particular criteria) are provided." ;
                 
                 rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#SingleSignOnPortal

:SingleSignOnPortal rdf:type owl:Class ;
                    
                    rdfs:label "Single Sign-On Portal" ;
                    
                    rdfs:subClassOf :FederationService ;
                    
                    rdfs:comment "A Single-Sign-on Portal, which provides web-based authentication and access to the authorized Clearinghouse services and other GENI user tools." ;
                    
                    rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#SliceAuthority

:SliceAuthority rdf:type owl:Class ;
                
                rdfs:label "Slice Authority"@en ;
                
                rdfs:subClassOf :FederationService ;
                
                rdfs:comment "A Slice Authority provides experimenters with slice credentials by which to invoke AM (Aggregate Manger) API calls on federation aggregates."@en ;
                
                rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://federation.av.tu-berlin.de/fiddle#TechnicalInfrastructure

:TechnicalInfrastructure rdf:type owl:Class ;
                         
                         rdfs:label "Technical Infrastructure" ;
                         
                         rdfs:subClassOf :Infrastructure ;
                         
                         rdfs:comment "technical infrastructures as technical systems built up of technical artifacts like “tools”, “machines”, “programs”, “buildings”, etc." ;
                         
                         rdfs:isDefinedBy "http://www.fi-infinity.eu/portal/resources/deliverable/d36" .



###  http://federation.av.tu-berlin.de/fiddle#WiredAccessNetwork

:WiredAccessNetwork rdf:type owl:Class ;
                    
                    rdfs:label "Wired Access Network" ;
                    
                    rdfs:subClassOf :TechnicalInfrastructure ;
                    
                    rdfs:seeAlso <http://www.fi-infinity.eu/portal/resources/deliverable/d36> .



###  http://federation.av.tu-berlin.de/fiddle#WirelessInfrastructure

:WirelessInfrastructure rdf:type owl:Class ;
                        
                        rdfs:label "Wireless Infrastructure" ;
                        
                        rdfs:subClassOf :TechnicalInfrastructure .



###  http://fp7-novi.eu/im.owl#Platform

novi:Platform rdf:type owl:Class ;
              
              rdfs:subClassOf nml:Group ;
              
              rdfs:comment "\"A Platform describes a particular testbed in the NOVI federation.\" http://www.semantic-web-journal.net/system/files/swj580.pdf"@en ;
              
              rdfs:seeAlso :Infrastructure .



###  http://schemas.ogf.org/nml/2013/05/base#Group

nml:Group rdf:type owl:Class ;
          
          rdfs:label "Group" ;
          
          rdfs:comment "\"A Group describes a collections of objects. Any object can be part of a group, including another Group. An object can also be part of multiple Groups.\" http://www.ogf.org/documents/GFD.206.pdf" .



###  http://schemas.ogf.org/nml/2013/05/base#Service

nml:Service rdf:type owl:Class ;
            
            rdfs:label "Service" ;
            
            rdfs:comment "Service describes an ability of the network. That is, it describes how the behavior can be changed dynamically." ;
            
            rdfs:seeAlso "http://www.ogf.org/documents/GFD.206.pdf" .



###  http://schemas.ogf.org/nml/base/2013/02#Node

<http://schemas.ogf.org/nml/base/2013/02#Node> rdf:type owl:Class ;
                                               
                                               rdfs:comment "\"A Node is generally a device connected to, or part of, the network. A Node does not necessarily correspond to a physical machine.\" http://www.ogf.org/documents/GFD.206.pdf" .



###  http://xmlns.com/foaf/0.1/Person

foaf:Person rdf:type owl:Class .





