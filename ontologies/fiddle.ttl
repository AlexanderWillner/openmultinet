                                           owl:versionInfo "0.1"^^xsd:string ;
                                           
                                           dc:date "2014-10-14"^^xsd:dateTime ;
                                           
                                           dc:description "A higher level ontology describing federated future internet infrastructures, based on work conducted at Open Multinet (http://open-multinet.info), OFG (NML; http://www.ogf.org/documents/GFD.206.pdf), Novi (http://www.semantic-web-journal.net/system/files/swj580.pdf), INDL (https://ivi.fnwi.uva.nl/sne/ndl/), NDL-OWL (https://geni-orca.renci.org/trac/wiki/NDL-OWL) and the Infinity Project (http://www.fi-infinity.eu)."^^xsd:string ;
                                           
