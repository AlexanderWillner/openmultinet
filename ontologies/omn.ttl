###  http://fp7-novi.eu/im.owl#Platform

novi:Platform rdf:type owl:Class ;
              
              owl:equivalentClass :Infrastructure ;
              
              rdfs:subClassOf nml:Group ;
              
              rdfs:comment "\"A Platform describes a particular testbed in the NOVI federation.\" http://www.semantic-web-journal.net/system/files/swj580.pdf"@en ;
              
              rdfs:seeAlso :Infrastructure .



###  http://open-multinet.info/ontology/omn-federation#AccountabilityService

:AccountabilityService rdf:type owl:Class ;
                       
                       rdfs:label "Accountability Service" ;
                       
                       rdfs:subClassOf nml:Service ;
                       
                       rdfs:comment "\"The Clearinghouse provides services that log transactions (successful or failed) between user tools and aggregates to support real-time and post-facto forensics analytics.\" http://groups.geni.net/geni/wiki/GeniClearinghouse" .



###  http://open-multinet.info/ontology/omn-federation#AggregateManager

:AggregateManager rdf:type owl:Class ;
                  
                  rdfs:label "Aggregate Manager"@en ;
                  
                  rdfs:subClassOf nml:Service ;
                  
                  rdfs:comment "An instance of an Aggregate Manager satisfying the Aggregate Manager API."@en .



###  http://open-multinet.info/ontology/omn-federation#ApplicationServiceDelivery

:ApplicationServiceDelivery rdf:type owl:Class ;
                            
                            rdfs:label "Application and Service Delivery" ;
                            
                            rdfs:subClassOf :TechnicalInfrastructure ;
                            
                            rdfs:comment "See http://www.fi-infinity.eu/portal/resources/deliverable/d36" .



###  http://open-multinet.info/ontology/omn-federation#AuthorizationService

:AuthorizationService rdf:type owl:Class ;
                      
                      rdfs:label "Authorization Service" ;
                      
                      rdfs:subClassOf nml:Service ;
                      
                      rdfs:comment "\"The Clearinghouse provides services to determine whether particular actions (within the Clearinghouse or with respect to a particular Aggregate) are permitted by federation policy.\" http://groups.geni.net/geni/wiki/GeniClearinghouse" .



###  http://open-multinet.info/ontology/omn-federation#BackboneNetwork

:BackboneNetwork rdf:type owl:Class ;
                 
                 rdfs:label "Backbone Network" ;
                 
                 rdfs:subClassOf :TechnicalInfrastructure ;
                 
                 rdfs:comment "See http://www.fi-infinity.eu/portal/resources/deliverable/d36" .



###  http://open-multinet.info/ontology/omn-federation#CloudComputing

:CloudComputing rdf:type owl:Class ;
                
                rdfs:label "Cloud Computing" ;
                
                rdfs:subClassOf :TechnicalInfrastructure ;
                
                rdfs:comment "See http://www.fi-infinity.eu/portal/resources/deliverable/d36 for further discussion." .



###  http://open-multinet.info/ontology/omn-federation#CustomerDevice

:CustomerDevice rdf:type owl:Class ;
                
                rdfs:label "Customer Device" ;
                
                rdfs:subClassOf :TechnicalInfrastructure ;
                
                rdfs:comment "See http://www.fi-infinity.eu/portal/resources/deliverable/d36" .



###  http://open-multinet.info/ontology/omn-federation#DataContentManagement

:DataContentManagement rdf:type owl:Class ;
                       
                       rdfs:label "Data and Content Management" ;
                       
                       rdfs:subClassOf :TechnicalInfrastructure ;
                       
                       rdfs:comment "See http://www.fi-infinity.eu/portal/resources/deliverable/d36" .



###  http://open-multinet.info/ontology/omn-federation#Federation

:Federation rdf:type owl:Class ;
            
            rdfs:label "Federation" ;
            
            rdfs:subClassOf <http://schema.org/Organization> ;
            
            rdfs:comment "A group of autonomous infrastructure/service providers agreeing upon standards of operation in a collective fashion."@en .



###  http://open-multinet.info/ontology/omn-federation#FederationService

:FederationService rdf:type owl:Class ;
                   
                   rdfs:label "Federation Service" ;
                   
                   rdfs:subClassOf nml:Service ;
                   
                   rdfs:comment "See http://groups.geni.net/geni/wiki/GeniClearinghouse" .



###  http://open-multinet.info/ontology/omn-federation#IdentityProvider

:IdentityProvider rdf:type owl:Class ;
                  
                  rdfs:label "Identity Provider" ;
                  
                  rdfs:subClassOf :FederationService ;
                  
                  rdfs:comment "\"An Identity Provider (IdP) provides certificates and PKI key materials to human users, registering them with the GENI federation as GENI users.\" http://groups.geni.net/geni/wiki/GeniClearinghouse" .



###  http://open-multinet.info/ontology/omn-federation#Infrastructure

:Infrastructure rdf:type owl:Class ;
                
                rdfs:label "Infrastructure" ;
                
                rdfs:subClassOf nml:Group ;
                
                rdfs:comment "An autonomous infrastrucure that offers resources/services to others within a federation. See also http://www.fi-infinity.eu/portal/resources/deliverable/d36 for discussion of what constitutes an ICT infrastructure."@en .



###  http://open-multinet.info/ontology/omn-federation#LoggingService

:LoggingService rdf:type owl:Class ;
                
                rdfs:label "Logging Service" ;
                
                rdfs:subClassOf :AccountabilityService ;
                
                rdfs:comment "The Logging Service provides the traceability between slivers and slices." ;
                
                rdfs:isDefinedBy <http://groups.geni.net/geni/wiki/GeniClearinghouse> .



###  http://open-multinet.info/ontology/omn-federation#OpenFlowInfrastructure

:OpenFlowInfrastructure rdf:type owl:Class ;
                        
                        rdfs:label "OpenFlow Infrastructure" ;
                        
                        rdfs:subClassOf :TechnicalInfrastructure .



