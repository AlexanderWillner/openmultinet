@prefix : <http://open-multinet.info/ontology/omn-federation#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix nml: <http://schemas.ogf.org/nml/2013/05/base#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix indl: <http://www.science.uva.nl/research/sne/indl#> .
@prefix novi: <http://fp7-novi.eu/im.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@base <http://open-multinet.info/ontology/omn-federation> .

<http://open-multinet.info/ontology/omn-federation> rdf:type owl:Ontology ;
                                                    
                                                    owl:versionInfo "0.5"^^xsd:string ;
                                                    
                                                    dc:date "2014-10-31"^^xsd:string ;
                                                    
                                                    dc:title "Open Multinet Infrastructure Description"^^xsd:string ;
                                                    
                                                    dc:license <http://creativecommons.org/licenses/by/4.0/> ;
                                                    
                                                    dc:creator :AlexanderWillner .



#################################################################
#
#    Object Properties
#
#################################################################


###  http://open-multinet.info/ontology/omn-federation#hasFederationMember

:hasFederationMember rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has federation member" ;
                     
                     rdfs:domain :Federation ;

                     owl:inverseOf :partOfFederation ;
                     
                     rdfs:range :FederationMember .




###  http://open-multinet.info/ontology/omn-federation#partOfFederation

:partOfFederation rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "is part of federation" ;
                  
                  rdfs:range :Federation ;

                  owl:inverseOf :hasFederationMember ;
                  
                  rdfs:domain :FederationMember .




###  http://open-multinet.info/ontology/omn-federation#administers

:administers rdf:type owl:ObjectProperty ;
             
             rdfs:label "administers" ;
             
             rdfs:range :Infrastructure ;

              owl:inverseOf :isAdministeredBy ;
             
             rdfs:domain foaf:Organization .




###  http://open-multinet.info/ontology/omn-federation#isAdministeredBy

:isAdministeredBy rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "is administered by" ;
                  
                  rdfs:domain :Infrastructure ;
                  
                  owl:inverseOf :administers ;
                  
                  rdfs:range foaf:Organization .




###  http://open-multinet.info/ontology/omn-federation#serves

:serves   rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "serves an infrastructure" ;
                      
                      rdfs:range :Infrastructure ;
                      
                      owl:inverseOf :hasService ;

                      rdfs:domain :Service .




###  http://open-multinet.info/ontology/omn-federation#hasService

:hasService   rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has service" ;
                      
                      rdfs:domain :Infrastructure ;

                      owl:inverseOf :serves ;
                      
                      rdfs:range :Service .




###  http://open-multinet.info/ontology/omn-federation#resourceOf

:resourceOf    rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is resource of an infrastructure" ;
                      
                      rdfs:range :Infrastructure ;

                      owl:inverseOf :hasResource ;
                      
                      rdfs:domain :Resource .




###  http://open-multinet.info/ontology/omn-federation#hasResource

:hasResource   rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has resource" ;
                      
                      rdfs:domain :Infrastructure ;

                      owl:inverseOf :resourceOf ;
                      
                      rdfs:range :Resource .




###  http://open-multinet.info/ontology/omn-federation#describedBy

:describedBy   rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "described by" ;
                      
                      rdfs:domain :Infrastructure ;
                      
                      rdfs:range void:Dataset .



#################################################################
#
#    Classes
#
#################################################################



###  http://open-multinet.info/ontology/omn-federation#Federation

:Federation rdf:type owl:Class ;
            
            rdfs:label "Federation" ;
            
            rdfs:subClassOf foaf:Organization .




###  http://open-multinet.info/ontology/omn-federation#FederationMember

:FederationMember rdf:type owl:Class ;
                  
                  rdfs:label "member of a federation" ;
                  
                  rdfs:subClassOf foaf:Organization .




###  http://open-multinet.info/ontology/omn-federation#Group

:Group       rdf:type owl:Class ;
                
                rdfs:label "Group" ;

                owl:equivalentClass novi:Group ;

                rdfs:seeAlso nml:Group .


                

###  http://open-multinet.info/ontology/omn-federation#Infrastructure

:Infrastructure rdf:type owl:Class ;
                
                rdfs:label "ICT infrastructure" ;

                owl:equivalentClass novi:Platform ;
                
                rdfs:subClassOf :Group .



###  http://open-multinet.info/ontology/omn-federation#Resource

:Resource       rdf:type owl:Class ;
                
                rdfs:label "Resource" ;

                owl:equivalentClass novi:Resource ;
                
                rdfs:seeAlso nml:NetworkObject .



###  http://open-multinet.info/ontology/omn-federation#Service

:Service       rdf:type owl:Class ;
                
                rdfs:label "Service" ;

                owl:equivalentClass novi:Service .




