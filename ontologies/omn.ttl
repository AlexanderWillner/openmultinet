#################################################################
#
#    Object Properties
#
#################################################################


###  http://open-multinet.info/ontology/omn-federation#hasInfrastructure

:hasInfrastructure rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "has infrastructure" ;
                   
                   rdfs:comment "Defines which infrastructures (aka testbeds) are part of a federation."@en ;
                   
                   rdfs:domain :Federation ;
                   
                   rdfs:range :Infrastructure ;
                   
                   owl:inverseOf :partOfFederation .



###  http://open-multinet.info/ontology/omn-federation#hasNode

:hasNode rdf:type owl:ObjectProperty ;
         
         rdfs:label "has a node" ;
         
         rdfs:comment "Defines which resources/services belongs to which infrastructure. Similar but not identical to NML hasNode (http://www.ogf.org/documents/GFD.206.pdf)."@en ;
         
         rdfs:domain :Infrastructure ;
         
         owl:inverseOf :partOfInfrastructure ;
         
         rdfs:seeAlso nml:hasNode ;
         
         rdfs:range <http://schemas.ogf.org/nml/base/2013/02#Node> .



###  http://open-multinet.info/ontology/omn-federation#hasService

:hasService rdf:type owl:ObjectProperty ;
            
            rdfs:label "has service" ;
            
            rdfs:comment "Defines which services a group (e.g. federation or infrastructure) offers (e.g. an GENI SFA Aggregate Manager). Similar but not identical to NML hasService (http://www.ogf.org/documents/GFD.206.pdf)."@en ;
            
            rdfs:seeAlso "http://schemas.ogf.org/nml/2013/05/base#hasService" ;
            
            rdfs:domain :Infrastructure ;
            
            rdfs:range nml:Service .



###  http://open-multinet.info/ontology/omn-federation#partOfFederation

:partOfFederation rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "is part of federation" ;
                  
                  rdfs:comment "Defines which federations an infrastructure belongs to."@en ;
                  
                  rdfs:range :Federation ;
                  
                  rdfs:domain :Infrastructure .



###  http://open-multinet.info/ontology/omn-federation#partOfInfrastructure

:partOfInfrastructure rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is part of an infrastructure" ;
                      
                      rdfs:comment "Defines which resources/services belongs to which infrastructure."@en ;
                      
                      rdfs:range :Infrastructure ;
                      
                      rdfs:domain <http://schemas.ogf.org/nml/base/2013/02#Node> .



###  http://xmlns.com/foaf/0.1/homepage

foaf:homepage rdf:type owl:ObjectProperty .



###  http://xmlns.com/foaf/0.1/mbox

foaf:mbox rdf:type owl:ObjectProperty .





#################################################################
#
#    Data properties
#
#################################################################


###  http://open-multinet.info/ontology/omn-federation#infraMontoring

:infraMontoring rdf:type owl:DatatypeProperty ;
                
                rdfs:label "has infra monitoring" ;
                
                rdfs:domain <http://schemas.ogf.org/nml/base/2013/02#Node> ;
                
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "no" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "yes" ;
                                                    rdf:rest rdf:nil
                                                  ]
                                       ]
                           ] .



###  http://open-multinet.info/ontology/omn-federation#omlServer

:omlServer rdf:type owl:DatatypeProperty ;
           
           rdfs:label "OML Server" ;
           
           rdfs:domain <http://schemas.ogf.org/nml/base/2013/02#Node> ;
           
           rdfs:range xsd:anyURI .



###  http://open-multinet.info/ontology/omn-federation#serviceURI

:serviceURI rdf:type owl:DatatypeProperty ;
            
            rdfs:label "has service URI" ;
            
            rdfs:comment "An endpoint where the service can be reached."@en ;
            
            rdfs:domain nml:Service ;
            
            rdfs:range xsd:anyURI .



###  http://xmlns.com/foaf/0.1/name

foaf:name rdf:type owl:DatatypeProperty .





