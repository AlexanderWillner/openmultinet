<http://schema.org/Organization> rdf:type owl:Class .



###  http://schemas.ogf.org/nml/2013/05/base#Group

nml:Group rdf:type owl:Class ;
          
          rdfs:label "Group" ;
          
          rdfs:subClassOf nml:NetworkObject ;
          
          rdfs:comment "\"A Group describes a collections of objects. Any object can be part of a group, including another Group. An object can also be part of multiple Groups.\" http://www.ogf.org/documents/GFD.206.pdf" .



###  http://schemas.ogf.org/nml/2013/05/base#NetworkObject

nml:NetworkObject rdf:type owl:Class ;
                  
                  rdfs:label "Network Object" ;
                  
                  rdfs:comment "\"The basic abstract class of the schema is the Network Object.\" http://www.ogf.org/documents/GFD.206.pdf" ;
                  
                  rdfs:seeAlso novi:Resource .



###  http://schemas.ogf.org/nml/2013/05/base#Service

nml:Service rdf:type owl:Class ;
            
            rdfs:label "Service" ;
            
            rdfs:subClassOf nml:NetworkObject ;
            
            rdfs:comment "\"Service describes an ability of the network. That is, it describes how the behavior can be changed dynamically.\" http://www.ogf.org/documents/GFD.206.pdf" ;
            
            rdfs:seeAlso novi:Service .



###  http://schemas.ogf.org/nml/base/2013/02#Node

<http://schemas.ogf.org/nml/base/2013/02#Node> rdf:type owl:Class ;
                                               
                                               rdfs:subClassOf nml:NetworkObject ;
                                               
                                               rdfs:comment "\"A Node is generally a device connected to, or part of, the network. A Node does not necessarily correspond to a physical machine.\" http://www.ogf.org/documents/GFD.206.pdf" ;
                                               
                                               rdfs:seeAlso novi:Node .



