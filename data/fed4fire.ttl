<http://federation.av.tu-berlin.de/fed4fire> rdf:type owl:Ontology ;
                                             
                                             rdfs:label "Fed4FIRE" ;
                                             
                                             owl:imports <http://open-multinet.info/ontology/omn-federation> ;

