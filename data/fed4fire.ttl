#    Individuals
#
#################################################################


###  http://federation.av.tu-berlin.de/fed4fire#fed4fire

:fed4fire rdf:type omn:Federation ,
                   owl:NamedIndividual ;
          
          rdfs:label "Fed4FIRE" ;
          
          rdfs:seeAlso <http://www.fed4fire.eu> ;
          
          omn:hasFederationMember   :FraunhoferFOKUS ,
                                    :iMinds  ;
          
          omn:administers :fed4firePortalInfrastructure ;
          
          foaf:logo <http://www.fed4fire.eu/uploads/media/fed4fire-logo.jpg> .





###  http://federation.av.tu-berlin.de/fed4fire#fed4firePortalInfrastructure

:fed4firePortalInfrastructure rdf:type omn:Infrastructure ,
                         owl:NamedIndividual ;
                
                rdfs:label "Fed4FIRE Portal Infrastructure"@en ;

                omn:hasService  :fed4firePortal ;

                omn:isAdministeredBy :fed4fire .

                


###  http://federation.av.tu-berlin.de/fed4fire#fed4firePortal

:fed4firePortal rdf:type sfa:SingleSignOnPortal ,
                         owl:NamedIndividual ;
                
                rdfs:label "Fed4FIRE Single Sign-On Portal"@en ;

                omn:serves :fed4firePortalInfrastructure ;
                
                sfa:serviceURI <http://portal.fed4fire.eu> .





###  http://federation.av.tu-berlin.de/fed4fire#FraunhoferFOKUS

:FraunhoferFOKUS  rdf:type owl:NamedIndividual ,
                omn:FederationMember ;

        omn:partOfFederation  :fed4fire;
       
        omn:administers :fusecoPlayground .





###  http://federation.av.tu-berlin.de/fed4fire#fusecoPlayground

:fusecoPlayground rdf:type omn:Infrastructure ,
                           owl:NamedIndividual ;
                  
                  rdfs:label "FUSECO"@en ;
                  
                  omn:isAdministeredBy  :FraunhoferFOKUS;
                  
                  omn:hasService :fpAggregateManager ,
                                 :fpSliceAuthority ;
                  
                  owl:sameAs <http://fuseco.fokus.fraunhofer.de/about#infrastructure> .





###  http://federation.av.tu-berlin.de/fed4fire#fpAggregateManager

:fpAggregateManager rdf:type sfa:AggregateManager ,
                             owl:NamedIndividual ;
                    
                    sfa:serviceURI <https://fuseco.fokus.fraunhofer.de:8443/sfa/api/am/v3> .





###  http://federation.av.tu-berlin.de/fed4fire#fpSliceAuthority

:fpSliceAuthority rdf:type sfa:SliceAuthority ,
                           owl:NamedIndividual ;
                  
                  sfa:serviceURI <https://fuseco.fokus.fraunhofer.de/api/sfa/registry/v1> .





###  http://federation.av.tu-berlin.de/fed4fire#iMinds

:iMinds rdf:type owl:NamedIndividual ,
                omn:FederationMember ;

        omn:partOfFederation  :fed4fire;
       
        omn:administers :virtualWall .





###  http://federation.av.tu-berlin.de/fed4fire#virtualWall

:virtualWall rdf:type omn:Infrastructure ,
                      owl:NamedIndividual ;
             
             rdfs:label "Virtual Wall"@en ;
             
             omn:isAdministeredBy :iMinds ;
             
             omn:hasService :vwAggregateManager ,
                            :vwSliceAuthority ;
             
             rdfs:seeAlso <http://www.iminds.be/en/succeed-with-digital-research/technical-testing> ;
             
             owl:sameAs <http://www.wall2.ilabt.iminds.be/about#infrastructure> .




###  http://federation.av.tu-berlin.de/fed4fire#vwAggregateManager

:vwAggregateManager rdf:type sfa:AggregateManager ,
                             owl:NamedIndividual ;
                    
                    sfa:serviceURI <https://www.wall2.ilabt.iminds.be:12369/protogeni/xmlrpc/am/3.0> .




###  http://federation.av.tu-berlin.de/fed4fire#vwSliceAuthority

:vwSliceAuthority rdf:type sfa:SliceAuthority ,
                           owl:NamedIndividual ;
                  
                  sfa:serviceURI <https://www.wall2.ilabt.iminds.be:12369/protogeni/xmlrpc/sa> .





